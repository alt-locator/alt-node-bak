var name = require('../build/config').Config.locationName;
var Logger = require('../build/logger').Logger;
var logger = new Logger('bin');

var AltFirebaseService = require('../build/tool/alt.service').AltFirebaseService;
AltFirebaseService.updateThis().then((result) => {
  if (result) {
    logger.debug('updated');
  } else {
    logger.debug('not updated');
  }
});
