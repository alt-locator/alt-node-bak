var name = require('../build/config').Config.locationName;
var Logger = require('top-banana-logger').Logger;
var logger = new Logger('bin');

var AltFirebaseService = require('../build/server/alt.service').AltFirebaseService;
AltFirebaseService.updateHost(name).then((result) => {
  if (result) {
    logger.debug('updated');
  } else {
    logger.debug('not updated');
  }
});
